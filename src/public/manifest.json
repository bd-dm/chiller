{
	"manifest_version": 3,
	"name": "Chiller",
	"description": "A Chrome extension that helps you fill anything with chill",
	"version": "0.0.1",
	"action": {
		"default_popup": "popup/index.html"
	},
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["content/index.js"]
		}
	],
	"web_accessible_resources": [{
		"matches": ["*://*/*"],
		"resources": ["assets-manifest.json", "assets/*"]
	}]
}
