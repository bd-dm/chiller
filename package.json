{
  "name": "chiller",
  "version": "0.0.33",
  "description": "A Chrome extension that helps you fill anything with chill",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "storybook": "storybook dev -p 6006",
    "build": "vite build --mode production",
    "test": "turbo test:e2e test:unit",
    "test:e2e": "playwright test",
    "test:unit": "jest",
    "lint": "turbo lint:code lint:types lint:styles",
    "lint:styles": "stylelint \"**/*.scss\"",
    "lint:code": "eslint . --ext .js,.ts,.tsx",
    "lint:types": "tsc --noEmit",
    "postinstall": "git config core.hooksPath '.git-hooks'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bd-dm/chiller.git"
  },
  "author": "Dmitrii Bardyshev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bd-dm/chiller/issues"
  },
  "homepage": "https://github.com/bd-dm/chiller#readme",
  "dependencies": {
    "draggabilly": "^3.0.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.0",
    "random-word-slugs": "^0.1.7",
    "solid-js": "^1.6.2",
    "sortablejs": "^1.15.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/blocks": "^7.0.9",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/chrome": "^0.0.202",
    "@types/draggabilly": "^2.1.3",
    "@types/lodash-es": "^4.17.6",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.11.9",
    "@types/sortablejs": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "babel": "^6.23.0",
    "babel-preset-solid": "^1.6.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-solid": "^0.8.0",
    "jest": "^29.3.1",
    "minimatch": "^5.1.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.3.0",
    "sass": "^1.56.1",
    "storybook": "^7.0.9",
    "storybook-solidjs": "^1.0.0-beta.2",
    "storybook-solidjs-vite": "^1.0.0-beta.2",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-node": "^10.9.1",
    "turbo": "^1.9.3",
    "typescript": "^4.9.3",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^3.2.4",
