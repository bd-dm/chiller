{
  "name": "chiller",
  "version": "0.0.35",
  "description": "A Chrome extension that helps you fill anything with chill",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "storybook": "storybook dev -p 6006",
    "build": "vite build --mode production",
    "test": "turbo test:e2e test:unit",
    "test:e2e": "playwright test",
    "test:unit": "jest",
    "lint": "turbo lint:code lint:types lint:styles",
    "lint:styles": "stylelint \"**/*.scss\"",
    "lint:code": "eslint . --ext .js,.ts,.tsx",
    "lint:types": "tsc --noEmit",
    "postinstall": "git config core.hooksPath '.git-hooks'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bd-dm/chiller.git"
  },
  "author": "Dmitrii Bardyshev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bd-dm/chiller/issues"
  },
  "homepage": "https://github.com/bd-dm/chiller#readme",
  "dependencies": {
    "@thisbeyond/solid-dnd": "^0.7.5",
    "draggabilly": "^3.0.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.4",
    "random-word-slugs": "^0.1.7",
    "solid-js": "^1.8.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/testing-library": "^0.2.2",
    "@types/chrome": "^0.0.259",
    "@types/draggabilly": "^2.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel": "^6.23.0",
    "babel-preset-solid": "^1.8.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-solid": "^0.13.1",
    "jest": "^29.7.0",
    "minimatch": "^9.0.3",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.9.6",
    "sass": "^1.70.0",
    "storybook": "^7.6.10",
    "storybook-solidjs": "^1.0.0-beta.2",
    "storybook-solidjs-vite": "^1.0.0-beta.2",
    "stylelint": "^16.2.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "ts-node": "^10.9.2",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^5.0.12",
    "vite-plugin-eslint": "^1.8.1",
