{
  "name": "chiller",
  "version": "0.0.1",
  "description": "A Chrome extension that helps you fill anything with chill",
  "scripts": {
    "dev": "turbo vite:dev",
    "build": "turbo vite:build",
    "build:live": "turbo vite:build:live",
    "preview": "turbo vite:preview",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:build:live": "vite build --watch",
    "vite:preview": "vite preview",
    "test": "jest",
    "lint:code": "eslint . --ext .js,.ts,.tsx",
    "lint:types": "tsc --noEmit",
    "lint": "npm run lint:code && npm run lint:types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bd-dm/chiller.git"
  },
  "author": "Dmitrii Bardyshev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bd-dm/chiller/issues"
  },
  "homepage": "https://github.com/bd-dm/chiller#readme",
  "dependencies": {
    "lodash-es": "^4.17.21",
    "solid-js": "^1.6.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/parser": "^5.43.0",
    "babel": "^6.23.0",
    "babel-preset-solid": "^1.6.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-solid": "^0.8.0",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "turbo": "^1.6.3",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-solid": "^2.4.0"
  }
}
